{
  "name": "@varandas/clash-royale-api",
  "version": "0.0.0-development",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/AndreVarandas/clash-royale-api",
  "author": "Andr√© Varandas <andre.m.varandas@gmail.com>",
  "license": "MIT",
  "keywords": [
    "clash",
    "royale",
    "api",
    "official",
    "wrapper"
  ],
  "bugs": {
    "url": "https://github.com/AndreVarandas/clash-royale-api/issues"
  },
  "homepage": "https://github.com/AndreVarandas/clash-royale-api#readme",
  "scripts": {
    "commit": "npx git-cz",
    "test": "jest --config jestconfig.json",
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "semantic-release": "semantic-release",
    "docs": "typedoc docs src"
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "@types/jest": "^24.0.0",
    "axios": "^0.18.0"
  },
  "files": [
    "lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint",
      "pre-push": "yarn test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
